			</div> 
			
			<div id='report'>
				<%= name.toUpperCase() %><br><br>
				
				<%= address1.toUpperCase() %><br><br>

				<%= address2.toUpperCase() %><br><br>
				
				<%= city.toUpperCase() %><br><br>
				
				<%= state %><br><br>
				
				<%= display_zipcode.toUpperCase() %>
			</div>
		
	</section>		
	
	<%- include ('../partials/footer'); -%>
</body>
</html>


